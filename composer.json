{
    "name": "fabpot/silex-skeleton",
    "description": "A pre-configured skeleton for the Silex microframework",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.0",
        "silex/web-profiler": "~1.0",
        "symfony/browser-kit": ">2.3,<2.8-dev",
        "symfony/class-loader": ">2.3,<2.8-dev",
        "symfony/config": ">2.3,<2.8-dev",
        "symfony/console": ">2.3,<2.8-dev",
        "symfony/css-selector": ">2.3,<2.8-dev",
        "symfony/debug": ">2.3,<2.8-dev",
        "symfony/finder": ">2.3,<2.8-dev",
        "symfony/form": ">2.3,<2.8-dev",
        "symfony/monolog-bridge": ">2.3,<2.8-dev",
        "symfony/process": ">2.3,<2.8-dev",
        "symfony/security": ">2.3,<2.8-dev",
        "symfony/translation": ">2.3,<2.8-dev",
        "symfony/twig-bridge": ">2.3,<2.8-dev",
        "symfony/validator": ">2.3,<2.8-dev",
        "jasongrimes/silex-simpleuser" : "*",
	    "miljar/php-exif": "~0.5.0",
        "symfony/yaml"  : ">2.3,<2.8-dev",
        "dflydev/doctrine-orm-service-provider" : "~1.0.7"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
